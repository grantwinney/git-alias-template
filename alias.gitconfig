[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  type = cat-file -t
  dump = cat-file -p

  psuh = push
  puhs = push
  shove2 = !git add . && git commit -m $1 && git push
  shove = !git add . && git commit -m \"$1\" && git push && :
